PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?anatomical_entity

WHERE
{
?anatomical_entity rdfs:subClassOf* <http://purl.obolibrary.org/obo/UBERON_0001062>
FILTER (
?anatomical_entity rdfs:definedBy <http://purl.obolibrary.org/obo/uberon.owl> ||
?anatomical_entity rdfs:definedBy <http://purl.obolibrary.org/obo/bspo.owl> ||
?anatomical_entity rdfs:definedBy <http://purl.obolibrary.org/obo/cl.owl>
)

}